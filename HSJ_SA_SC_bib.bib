@article{bailey_new_2020,
	title = {A New Physically-Based Spatially-Distributed Groundwater Flow Module for {SWAT}+},
	volume = {7},
	rights = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2306-5338},
	url = {https://www.mdpi.com/2306-5338/7/4/75},
	doi = {10.3390/hydrology7040075},
	abstract = {Watershed models are used worldwide to assist with water and nutrient management under conditions of changing climate, land use, and population. Of these models, the Soil and Water Assessment Tool ({SWAT}) and {SWAT}+ are the most widely used, although their performance in groundwater-driven watersheds can sometimes be poor due to a simplistic representation of groundwater processes. The purpose of this paper is to introduce a new physically-based spatially-distributed groundwater flow module called gwflow for the {SWAT}+ watershed model. The module is embedded in the {SWAT}+ modeling code and is intended to replace the current {SWAT}+ aquifer module. The model accounts for recharge from {SWAT}+ Hydrologic Response Units ({HRUs}), lateral flow within the aquifer, Evapotranspiration ({ET}) from shallow groundwater, groundwater pumping, groundwater–surface water interactions through the streambed, and saturation excess flow. Groundwater head and groundwater storage are solved throughout the watershed domain using a water balance equation for each grid cell. The modified {SWAT}+ modeling code is applied to the Little River Experimental Watershed ({LREW}) (327 km2) in southern Georgia, {USA} for demonstration purposes. Using the gwflow module for the {LREW} increased run-time by 20\% compared to the original {SWAT}+ modeling code. Results from an uncalibrated model are compared against streamflow discharge and groundwater head time series. Although further calibration is required if the {LREW} model is to be used for scenario analysis, results highlight the capabilities of the new {SWAT}+ code to simulate both land surface and subsurface hydrological processes and represent the watershed-wide water balance. Using the modified {SWAT}+ model can provide physically realistic groundwater flow gradients, fluxes, and interactions with streams for modeling studies that assess water supply and conservation practices. This paper also serves as a tutorial on modeling groundwater flow for general watershed modelers.},
	pages = {75},
	number = {4},
	journaltitle = {Hydrology},
	author = {Bailey, Ryan T. and Bieger, Katrin and Arnold, Jeffrey G. and Bosch, David D.},
	urldate = {2023-04-27},
	date = {2020-12},
	langid = {english},
	note = {Number: 4
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {groundwater, {SWAT}+, groundwater–surface water interactions, modeling},
	file = {Full Text PDF:C\:\\Users\\Alejandro\\Zotero\\storage\\E45GR9MW\\Bailey et al. - 2020 - A New Physically-Based Spatially-Distributed Groun.pdf:application/pdf},
}

@inproceedings{schurz_analysis_2022,
	location = {Prague},
	title = {Analysis of the spatiotemporal variability of simulated water balance components and related {SWAT}+ model parameters in seven German catchments},
	url = {https://swatconference.tamu.edu/#/book-of-abstracts/2022-prague},
	abstract = {{SWAT}+ is a fundamentally restructured version of the Soil and Water Assessment Tool. The new model structure affects the functional relationship between some of the simulated processes and their associated model parameters. This study provides an overview of process-parameter relationships based on a multi-site analysis. Within the framework of a cooperation of scientists working with the {SWAT} model in Germany ({SWAT}-{DE}), {SWAT}+ was applied to seven catchments, located in different climatic and topographic regions in Germany.  We investigated the functional relationships between {SWAT}+ model parameters and different simulated water balance components.

We employed {SWAT}+ revision 60.5.3 to simulate snowfall processes, evapotranspiration, the surface and lateral runoff components including tile drainage flow, and groundwater related processes at the daily time temporal resolution for all catchments. A selection of 22 {SWAT}+ model parameters relevant for the simulation of the water cycle were sampled with a Fourier Amplitude Sensitivity Test ({FAST}) sampling design and subsequently input into the {SWAT}+ simulations. {FAST} sensitivity indices were calculated for each time step of the simulated water balance components. The daily {FAST} sensitivities were summarized for each process-parameter relationship for each catchment . We identified sensitivity patterns that were found to be common between catchments. Other process-parameter relationships were exclusively relevant in individual catchments. The analysis determined several functional parameter groups that can be exclusively associated with a specific hydrological process. In contrast, parameters such as perc or awc were found to be dominant across multiple processes. The findings derived from this study support a more process-based and systematic {SWAT}+ model calibration.},
	eventtitle = {2022 {SWAT} Conference},
	booktitle = {2022 Prague {SWAT} Conference},
	author = {Schürz, Christoph and Bauwe, Andreas and Bieger, Katrin and Julich, Stefan and Kiesel, Jens and Mehdi-Schulz, Bano and Rathjens, Hendrik and Schaffhauser, Timo and Strauch, Michael and Wagner, Paul D.},
	date = {2022-07-14},
}

